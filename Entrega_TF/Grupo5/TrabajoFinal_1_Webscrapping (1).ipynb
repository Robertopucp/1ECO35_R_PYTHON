  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "3277a8dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "upublica = datosn[ datosn.Tipo == 'pública']\n",
    "uprivada = datosn[ datosn.Tipo == 'privada']"
   ]
